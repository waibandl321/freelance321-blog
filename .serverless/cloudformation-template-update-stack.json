        "TargetFunctionArn": {
          "Fn::GetAtt": [
            "NuxtSsrCoreLambdaFunction",
            "Arn"
          ]
        }
      }
    },
    "NuxtSsrCoreLambdaPermissionFnUrl": {
      "Type": "AWS::Lambda::Permission",
      "Properties": {
        "FunctionName": {
          "Fn::GetAtt": [
            "NuxtSsrCoreLambdaFunction",
            "Arn"
          ]
        },
        "Action": "lambda:InvokeFunctionUrl",
        "Principal": "*",
        "FunctionUrlAuthType": "NONE"
      }
    }
  },
  "Outputs": {
    "ServerlessDeploymentBucketName": {
      "Value": {
        "Ref": "ServerlessDeploymentBucket"
      },
      "Export": {
        "Name": "sls-freelance321-blog-dev-ServerlessDeploymentBucketName"
      }
    },
    "NuxtSsrCoreLambdaFunctionQualifiedArn": {
      "Description": "Current Lambda function version",
      "Value": {
        "Ref": "NuxtSsrCoreLambdaVersionMndWq8tO4OhYSchy8RZAyBNll97NgF0UGq7qxXMSw"
      },
      "Export": {
